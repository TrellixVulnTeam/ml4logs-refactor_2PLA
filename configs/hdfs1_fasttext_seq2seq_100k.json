{
    "pipeline": [
        {
            "action": "download",
            "url": "https://zenodo.org/record/3227177/files/HDFS_1.tar.gz",
            "path": "data/raw/HDFS1.tar.gz"
        },
        {
            "action": "extract",
            "in_path": "data/raw/HDFS1.tar.gz",
            "out_dir": "data/raw/HDFS1"
        },
        {
            "action": "head",
            "logs_path": "data/raw/HDFS1/HDFS.log",
            "logs_head_path": "data/raw/HDFS1/HDFS.log",
            "n_rows": 100000
        },
        {
            "action": "prepare",
            "dataset": "HDFS1",
            "in_dir": "data/raw/HDFS1",
            "logs_path": "data/interim/HDFS1/logs.txt",
            "blocks_path": "data/interim/HDFS1/blocks.npy",
            "labels_path": "data/interim/HDFS1/labels.npy",
            "timedeltas_path": "data/interim/HDFS1/timedeltas.npy"
        },
        {
            "action": "train_fasttext",
            "logs_path": "data/interim/HDFS1/logs.txt",
            "model_path": "models/embeddings/fasttext_skipgram-hdfs1.bin",
            "model_args": {
                "dim": 100,
                "minCount": 10000,
                "minn": 1,
                "maxn": 1,
                "thread": 1,
                "verbose": 2
            }
        },
        {
            "action": "preprocess_fasttext",
            "logs_path": "data/interim/HDFS1/logs.txt",
            "model_path": "models/embeddings/fasttext_skipgram-hdfs1.bin",
            "embeddings_path": "data/interim/HDFS1/fasttext.npy"
        },
        {
            "action": "merge_features",
            "features_paths": [
                "data/interim/HDFS1/fasttext.npy",
                "data/interim/HDFS1/timedeltas.npy"
            ],
            "merged_path": "data/interim/HDFS1/fasttext-timedeltas.npy"
        },
        {
            "action": "train_test_seq2seq",
            "features_path": "data/interim/HDFS1/fasttext-timedeltas.npy",
            "blocks_path": "data/interim/HDFS1/blocks.npy",
            "labels_path": "data/interim/HDFS1/labels.npy",
            "stats_path": "reports/results/HDFS1-skipgram-seq2seq-100k.json",
            "model_kwargs": {
                "n_lstm_layers": 1,
                "n_hidden_linears": 2,
                "linear_width": 300,
                "linear_norm": false
            },
            "optim_kwargs": {
                "lr": 1e-2
            },
            "lr_scheduler_kwargs": {
                "gamma": 0.9817
            },
            "train_size": 0.4,
            "validation_size": 0.2,
            "batch_size": 512,
            "epochs": 5,
            "device": "cuda",
            "seed": 14235
        }
    ]
}